{
    "manifest_version": 3,
    "name": "Coordinate Clicker",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs"
    ],
    "host_permissions": ["<all_urls>"]
  }
  